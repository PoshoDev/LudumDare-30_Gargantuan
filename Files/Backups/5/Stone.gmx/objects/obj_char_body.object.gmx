            <string>///Movement

//Makes it move depending speed_x/y
x +=    (speed_x)
y +=    (speed_y)

//Adjusts speed depending the button press
if damage != 3
{
if keyboard_check(ord("W"))
    {
    speed_y -=  0.5*flow
    if gas_able = 1
        {
        gas_able = 0
        instance_create(x,y,obj_gas)
        alarm[1] = 5
        }
    }

if keyboard_check(ord("S"))
    {
    speed_y +=  0.5*flow
    if gas_able = 1
        {
        gas_able = 0
        instance_create(x,y,obj_gas)
        alarm[1] = 5
        }
    }
    
if keyboard_check(ord("A"))
    {
    speed_x -=  0.5*flow
    if gas_able = 1
        {
        gas_able = 0
        instance_create(x,y,obj_gas)
        alarm[1] = 5
        }
    }

if keyboard_check(ord("D"))
    {
    speed_x +=  0.5*flow
    if gas_able = 1
        {
        gas_able = 0
        instance_create(x,y,obj_gas)
        alarm[1] = 5
        }
    }
}
    
//Slows down if key is released
if speed_x &gt;    0
    speed_x -=  0.1
if speed_x &lt;    0
    speed_x +=  0.1
if speed_y &gt;    0
    speed_y -=  0.1
if speed_y &lt;    0
    speed_y +=  0.1
    
//Limits its speed
if speed_x &gt;    7
    speed_x =   7
if speed_x &lt;    -7
    speed_x =   -7
if speed_y &gt;    7
    speed_y =   7
if speed_y &lt;    -7
    speed_y =   -7
